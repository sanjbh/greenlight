[req]
  default_bits = 2048
  prompt = no
  default_md = sha256
  req_extensions = req_ext
  distinguished_name = dn
  
  [dn]
  C = US
  ST = State
  L = City
  O = Organization
  CN = vault.local
  
  [req_ext]
  subjectAltName = @alt_names
  
  [alt_names]
  DNS.1 = vault
  DNS.2 = vault.local
  DNS.3 = localhost
  DNS.4 = vault-greenlight
  IP.1 = 127.0.0.1
